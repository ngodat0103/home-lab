# Database Configuration
VAULTWARDEN_DB_TYPE=postgres
VAULTWARDEN_DB_HOST=localhost
VAULTWARDEN_DB_PORT=5432
VAULTWARDEN_DB_NAME=vaultwarden
VAULTWARDEN_DB_USERNAME=vaultwarden
VAULTWARDEN_DB_PASSWORD=changeme

# Vaultwarden Configuration
VAULTWARDEN_DATA_DIR=/app/data/vaultwarden
VAULTWARDEN_ADMIN_TOKEN=changeme

# S3/MinIO Configuration
S3_BUCKET=vaultwarden-backups
S3_ENDPOINT=https://your-s3-endpoint.com
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
AWS_REGION=auto

# MinIO Local Development
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin

# Backup Configuration
BACKUP_PASSWORD=your-strong-encryption-password

# Optional: Timeout Configuration (in seconds)
PGDUMP_TIMEOUT=300
PGRESTORE_TIMEOUT=600

